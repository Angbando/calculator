<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/dogsss"
    tools:context=".MainActivity">

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/_1"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@id/button2"
        tools:ignore="MissingConstraints,TextContrastCheck"
        tools:layout_editor_absoluteY="234dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#2AFF5722"
        android:text="@string/_2"
        android:textColor="#8B4513"
        android:textSize="20sp"
        tools:ignore="MissingConstraints,TextContrastCheck"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="234dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/_3"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@id/button2"
        tools:ignore="MissingConstraints,TextContrastCheck"
        tools:layout_editor_absoluteY="234dp" />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/_4"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@id/button1"
        app:layout_constraintStart_toStartOf="@id/button1"
        app:layout_constraintTop_toBottomOf="@id/button1"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/_5"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@id/button2"
        app:layout_constraintStart_toStartOf="@id/button2"
        app:layout_constraintTop_toBottomOf="@id/button2"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/_6"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@id/button3"
        app:layout_constraintStart_toStartOf="@id/button3"
        app:layout_constraintTop_toBottomOf="@id/button3"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/button7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/_7"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@id/button4"
        app:layout_constraintStart_toStartOf="@id/button4"
        app:layout_constraintTop_toBottomOf="@id/button4"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/_8"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@id/button5"
        app:layout_constraintStart_toStartOf="@id/button5"
        app:layout_constraintTop_toBottomOf="@id/button5"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/button9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/_9"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@id/button6"
        app:layout_constraintStart_toStartOf="@id/button6"
        app:layout_constraintTop_toBottomOf="@id/button6"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/button0"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/_0"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@id/button8"
        app:layout_constraintStart_toStartOf="@id/button8"
        app:layout_constraintTop_toBottomOf="@id/button8"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/comma"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#2AFF5722"
        android:text="@string/comma"
        android:textColor="#8B4513"
        android:textSize="20sp"
        tools:ignore="MissingConstraints,TextContrastCheck"
        tools:layout_editor_absoluteX="161dp"
        tools:layout_editor_absoluteY="467dp" />

    <Button
        android:id="@+id/plus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/plus"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@id/comma"
        tools:ignore="MissingConstraints,TextContrastCheck"
        tools:layout_editor_absoluteY="467dp" />

    <Button
        android:id="@+id/minus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/minus"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@id/comma"
        tools:ignore="MissingConstraints,TextContrastCheck"
        tools:layout_editor_absoluteY="467dp" />

    <Button
        android:id="@+id/equals"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/equals"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintTop_toBottomOf="@id/comma"
        tools:ignore="MissingConstraints,TextContrastCheck"
        tools:layout_editor_absoluteX="161dp" />

    <Button
        android:id="@+id/multiply"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/multiply"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintStart_toEndOf="@id/equals"
        app:layout_constraintTop_toBottomOf="@id/plus"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/divide"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:layout_marginEnd="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/divide"
        android:textColor="#8B4513"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@id/equals"
        app:layout_constraintTop_toBottomOf="@id/minus"
        tools:ignore="TextContrastCheck" />

    <Button
        android:id="@+id/del"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/del"
        android:textColor="#8B4513"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@id/equals"
        app:layout_constraintStart_toEndOf="@id/divide"
        app:layout_constraintTop_toBottomOf="@id/divide" />

    <Button
        android:id="@+id/delete"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:backgroundTint="#2AFF5722"
        android:text="@string/delete"
        android:textColor="#8B4513"
        android:textSize="16sp"
        app:layout_constraintEnd_toStartOf="@id/multiply"
        app:layout_constraintStart_toEndOf="@id/equals"
        app:layout_constraintTop_toBottomOf="@id/multiply" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="cursive"
        android:text="@string/credits"
        android:textColor="#8B4513"
        android:textSize="30sp"
        tools:layout_editor_absoluteX="105dp"
        tools:layout_editor_absoluteY="668dp"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>